@model AdminRole.Models.AssignDevTicketModel

@{
    ViewBag.Title = "AssignUsers";
}

<div class="content-main">
    <div class="banner">
        <h2>
            <a href="index.html">Home</a>
            <i class="fa fa-angle-right"></i>
            <span>Projects</span>
        </h2>
        </div>
        <div class="blank">
            <div class="blank-page">
                <h2>Assign users</h2>
                <hr />
                <section class="content">
                    @using (Html.BeginForm())
                    {<div class="form-group">
                            @Html.HiddenFor(p => p.TicketId)
                            @Html.DropDownListFor(p => p.SelectedDeveloperId, Model.DeveloperList, htmlAttributes: new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.DeveloperList, "", new { @class = "text-danger" })
                            <input type="submit" value="Submit!" class="btn btn-success" />
                           
                     </div>
                    }
                 </section>
             </div>
        </div>
</div>
